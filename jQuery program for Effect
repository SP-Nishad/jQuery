<html>
    <head>
        <link rel="stylesheet" href="css/bootstrap.min.css">
        <link rel="stylesheet" href="css/font-awesome.min.css">
        <script src="js/jquery-3.3.1.min.js"></script>
        <script>
        $(document).ready(function(){
        $("#btn1").click(function(){
        $("img").hide(5000);
        })
        $("#btn2").click(function(){
        $("img").show(5000);
        })
        $("#btn3").click(function(){
        $("img").fadeIn(5000);
        })
        $("#btn4").click(function(){
        $("img").fadeOut(5000);
        })
        $("#btn5").click(function(){
        $("img").fadeTo(5000);
        })
        $("#btn6").click(function(){
        $("img").fadeToggle("show");
        })
        $("#btn7").click(function(){
        $("img").slideUp(5000);
        })
        $("#btn8").click(function(){
        $("img").slideDown(5000);
        })
        $("#btn9").click(function(){
        $("img").slideToggle(5000);
        })
        })
        </script>
    </head>
    <body>
    <div class="container">
    <div class="row">
    <div class="col-sm-12">
    <div class="col-sm-4"></div>
    <div class="col-sm-4">
    <input type="button" value="hide()" id="btn1" class="form-control"> 
    <input type="button" value="show()" id="btn2" class="form-control">
    <input type="button" value="fadeIn()" id="btn3" class="form-control"> 
    <input type="button" value="fadeOut()" id="btn4" class="form-control"> 
    <input type="button" value="fadeTo()" id="btn5" class="form-control"> 
    <input type="button" value="fadeToggle()" id="btn6" class="form-control"> 
    <input type="button" value="slideUp()" id="btn7" class="form-control"> 
    <input type="button" value="slideDown()" id="btn8" class="form-control"> 
    <input type="button" value="slideToggle()" id="btn9" class="form-control"> 
    <input type="button" value="animate()" id="btn10" class="form-control"><br> 
