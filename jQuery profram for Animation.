<html>
<head>
<script src="js/jquery-3.3.1.min.js"></script>
<script>
$(document).ready(function(){
$("#one").mouseenter(function(){
$("#one").animate({top:"0px",left:"80%"},2000);
$("#one").animate({top:"300px",left:"80%"},2000);
$("#one").animate({top:"300px",left:"0%"},2000);
$("#one").animate({top:"0px",left:"0%"},2000);
$("#two").animate({top:"0px",left:"60%"},2000);
$("#two").animate({top:"200px",left:"30%"},2000);
$("two").animate({top:"300px",left:"70%"},2000);
$("#two").animate({top:"0px",left:"0%"},2000);
})
})
</script>
</head>
